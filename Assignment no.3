 */

//the preprocessor segment including the required files during the compilatuion
#include <iostream>
//additional namespaces requiured
using namespace std;
//Main Driving code
int main() 
{
  //Integer values
  int array[10],i,j;
  //user instructions for input
cout<<"enter the array elements:\n";
  for(int i=0;i<10;i++)
  {
    cin>>array[i];//reading the elements in to array
  }
//10	31	2	73	65	6	87	18	9

  //prints the first and the last on one line the second and the 9th on the next line the 3rd and the 7th on the next line and so forth.
  cout<<"Print the array elements:\n";
  for(int i=0,j=9;i<j;i++,j--)
    {
       cout<< array[i]<<"\t"<<array[j]<<"\n";
    }
return 0;
}
